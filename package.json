{
  "name": "threlte-spline",
  "description": "Helper function to load Spline scenes into threlte",
  "license": "MIT",
  "version": "0.3.0",
  "repository": {
    "url": "https://github.com/hongkiulam/threlte-spline"
  },
  "author": {
    "name": "Hong-Kiu Lam",
    "url": "https://github.com/hongkiulam"
  },
  "keywords": [
    "threejs",
    "svelte",
    "spline",
    "threlte",
    "splinetool"
  ],
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.ts --format esm --dts",
    "lint": "tsc",
    "ci": "pnpm lint && pnpm build",
    "release": "pnpm ci && changeset publish",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@splinetool/loader": "^0.9.235",
    "@types/three": "^0.149.0",
    "prettier": "^2.8.4",
    "three": "^0.150.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@splinetool/loader": ">=0.9 <1",
    "@types/three": ">=0.1"
  },
  "peerDependenciesMeta": {
    "@types/three": {
      "optional": true
    }
  }
}
